<?xml version="1.0" encoding="UTF-8"?>
<!-- Created with Cambalache 0.12.1 -->
<interface>
  <requires lib="gtk" version="4.10"/>
  <object class="GtkFileFilter" id="acceptableAudioFiles">
    <mime-types>
      <mime-type>audio/mpeg</mime-type>
      <mime-type>audio/ogg</mime-type>
      <mime-type>audio/flac</mime-type>
      <mime-type>audio/vorbis</mime-type>
    </mime-types>
  </object>
  <object class="GtkFileChooserNative" id="audioFileChooser">
    <property name="filter">acceptableAudioFiles</property>
    <property name="title">Select Audio File to Play</property>
    <property name="modal">1</property>
  </object>
  <object class="GtkAdjustment" id="playbackAdj">
    <property name="upper">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="volumeAdj">
    <property name="upper">100</property>
    <property name="value">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkWindow" id="Jukebox">
    <property name="title" translatable="1">Bassoon Jukebox</property>
    <property name="child">
      <object class="GtkBox">
        <property name="margin-top">4</property>
        <property name="margin-bottom">4</property>
        <property name="spacing">4</property>
        <child>
          <object class="GtkBox">
            <property name="hexpand">1</property>
            <property name="width-request">300</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkBox">
                <property name="height-request">157</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkButton" id="audioFileChooserButton">
                    <property name="focusable">1</property>
                    <property name="receives-default">1</property>
                    <child>
                      <object class="GtkBox">
                        <child>
                          <object class="GtkLabel">
                            <property name="label" translatable="1">(None)</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkImage">
                            <property name="icon-name">document-open</property>
                            <property name="use-fallback">1</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <placeholder/>
                </child>
                <child>
                  <placeholder/>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <child>
                  <object class="GtkLabel" id="playbackTimeLabel">
                    <property name="label" translatable="1">00:00:00</property>
                  </object>
                </child>
                <child>
                  <object class="GtkScale" id="playbackSlider">
                    <property name="hexpand">1</property>
                    <property name="focusable">1</property>
                    <property name="adjustment">playbackAdj</property>
                    <property name="show-fill-level">1</property>
                    <property name="round-digits">1</property>
                    <property name="digits">0</property>
                    <property name="value-pos">left</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <child>
                  <object class="GtkButton" id="rewindButton">
                    <property name="label" translatable="1">|&lt;</property>
                    <property name="focusable">1</property>
                    <property name="receives-default">1</property>
                  </object>
                </child>
                <child>
                  <object class="GtkToggleButton" id="playButton">
                    <property name="hexpand">1</property>
                    <property name="label" translatable="1">&gt;</property>
                    <property name="focusable">1</property>
                    <property name="receives-default">1</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="statusLabel">
                <property name="label" translatable="1">Bassoon Jukebox</property>
                <property name="xalign">0</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="halign">center</property>
            <property name="width-request">50</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkLabel">
                <property name="valign">center</property>
                <property name="label" translatable="1">Vol.</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="volumeLevelLabel">
                <property name="valign">center</property>
                <property name="label" translatable="1">100%</property>
              </object>
            </child>
            <child>
              <object class="GtkScale" id="volumeSlider">
                <property name="vexpand">1</property>
                <property name="focusable">1</property>
                <property name="orientation">vertical</property>
                <property name="adjustment">volumeAdj</property>
                <property name="inverted">1</property>
                <property name="show-fill-level">1</property>
                <property name="restrict-to-fill-level">0</property>
                <property name="fill-level">100</property>
                <property name="round-digits">2</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
